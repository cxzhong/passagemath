cd src

# Avoid running autotools. Patch no_vendor_cddlib.patch makes changes
# both to input and output.
touch configure **/Makefile.in

sdh_configure \
    $SAGE_CONFIGURE_GMP \
    --with-cdd="$SAGE_LOCAL"
sdh_make

$MAKE install-strip
if [ $? -ne 0 ]; then
   echo "Error installing TOPCOM."
   exit 1
fi
